theme="shapeshifter" # Remember to edit stows/neovim/.config/nvom/config/ui.vim
variant="dark"

base16_shell="$HOME/.dotfiles/externals/base16-shell/base16-$theme.$variant.sh"
[[ -s $base16_shell ]] && source $base16_shell

unset base16_shell
